    {
      "name": "top_5_target_ips",
      "description": "5 Top target IP addresses found ",
      "command": {
        "size": 0,
        "aggregations" : {
          "target_ips" : {
            "terms" : { 
              "field" : "flow_logs.target_ip.keyword",  
              "size" : 5 
              }
          }
        }
      },
      "output": [
